      "id": "download-file",
      "name": "Download Receipt File",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [460, 300]
    },
    {
      "parameters": {
        "operation": "analyze",
        "imageInput": "binary",
        "prompt": "Analiza este comprobante de pago y extrae la siguiente información en formato JSON:\\n- date: fecha del comprobante (formato YYYY-MM-DD)\\n- amount: monto total (solo número, sin símbolos)\\n- type: tipo de comprobante (factura_a, factura_b, factura_c, recibo, etc.)\\n- number: número del comprobante\\n- cuit: CUIT del emisor (si está presente)\\n- paymentMethod: método de pago (transferencia, efectivo, tarjeta, etc.)\\n- description: descripción del servicio o concepto\\n- confidence: nivel de confianza en la extracción (0-1)\\n\\nResponde SOLO con el JSON, sin texto adicional.",
        "options": {
          "maxTokens": 500,
          "temperature": 0.1
        }
      },
      "id": "openai-vision",
      "name": "OpenAI Vision Analysis",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [680, 300],
      "credentials": {
        "openAiApi": {
          "id": "openai-credentials",
          "name": "OpenAI API Key"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Parse the OpenAI response and validate data\\nconst response = $input.first().json.message.content;\\n\\ntry {\\n  const extractedData = JSON.parse(response);\\n  \\n  // Validate and clean the extracted data\\n  const cleanedData = {\\n    date: extractedData.date || new Date().toISOString().split('T')[0],\\n    amount: parseFloat(extractedData.amount) || 0,\\n    type: extractedData.type || 'comprobante',\\n    number: extractedData.number || 'N/A',\\n    cuit: extractedData.cuit || '',\\n    paymentMethod: extractedData.paymentMethod || 'no_especificado',\\n    description: extractedData.description || '',\\n    confidence: extractedData.confidence || 0.8\\n  };\\n  \\n  return {\\n    receiptId: $('Webhook Trigger').first().json.body.receiptId,\\n    extractedData: cleanedData,\\n    originalFileUrl: $('Webhook Trigger').first().json.body.fileUrl,\\n    success: true\\n  };\\n  \\n} catch (error) {\\n  return {\\n    receiptId: $('Webhook Trigger').first().json.body.receiptId,\\n    error: error.message,\\n    success: false\\n  };\\n}"
      },
      "id": "process-data",
      "name": "Process Extracted Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [900, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "success-condition",
              "leftValue": "={{ $json.success }}",
              "rightValue": true,
              "operator": {
                "type": "boolean",
                "operation": "equal"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "success-check",
      "name": "Success Check",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [1120, 300]
    },
    {
