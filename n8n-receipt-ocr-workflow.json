        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "receipt_date",
              "value": "={{ $json.extractedData.date }}"
            },
            {
              "name": "amount",
              "value": "={{ $json.extractedData.amount }}"
            },
            {
              "name": "receipt_type",
              "value": "={{ $json.extractedData.type }}"
            },
            {
              "name": "receipt_number",
              "value": "={{ $json.extractedData.number }}"
            },
            {
              "name": "patient_cuit",
              "value": "={{ $json.extractedData.cuit }}"
            },
            {
              "name": "payment_method",
              "value": "={{ $json.extractedData.paymentMethod }}"
            },
            {
              "name": "extracted_data",
              "value": "={{ JSON.stringify($json.extractedData) }}"
            },
            {
              "name": "extraction_status",
              "value": "extracted"
            },
            {
              "name": "validation_status",
              "value": "needs_review"
            },
            {
              "name": "validation_notes",
              "value": "Procesado automáticamente por n8n + OpenAI Vision. Confianza: {{ $json.extractedData.confidence }}"
            }
          ]
        },
        "options": {}
      },
