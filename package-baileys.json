
{
  "name": "baileys-whatsapp-server",
  "version": "2.0.0",
  "description": "Servidor optimizado de WhatsApp con Baileys para sistema de notificaciones",
  "main": "baileys-server-optimized.js",
  "scripts": {
    "start": "node baileys-server-optimized.js",
    "dev": "nodemon baileys-server-optimized.js",
    "pm2:start": "pm2 start baileys-server-optimized.js --name baileys-server",
    "pm2:stop": "pm2 stop baileys-server",
    "pm2:restart": "pm2 restart baileys-server",
    "pm2:logs": "pm2 logs baileys-server"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "whatsapp",
    "baileys",
    "nodejs",
    "notifications",
    "api"
  ],
  "author": "Sistema de Notificaciones",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
